-  private ParsedTask getOrMakeTask(TaskType type, String taskIDname,
-      boolean allowCreate) {
-    Map<String, ParsedTask> taskMap = otherTasks;
-    List<LoggedTask> tasks = this.result.getOtherTasks();
-
-    switch (type) {
