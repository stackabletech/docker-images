-  private static final Pattern heapPattern =
-      Pattern.compile("-Xmx([0-9]+)([mMgG])");
-
-  final int heapMegabytes;
-
-  final String queue;
-  final String jobName;
-
-  final int clusterMapMB;
-  final int clusterReduceMB;
