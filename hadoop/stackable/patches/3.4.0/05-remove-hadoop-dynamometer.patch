-  public static final String AUDIT_LOG_PARSE_REGEX_KEY =
-      "auditreplay.log-parse-regex";
-  public static final String AUDIT_LOG_PARSE_REGEX_DEFAULT =
-      "^(?<timestamp>.+?) INFO [^:]+: (?<message>.+)$";
