{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "local>stackabletech/.github:renovate-config",
    "docker:pinDigests"
  ],
  "packageRules": [
    {
      "matchDatasources": [
        "docker"
      ],
      "registryUrls": [
        "https://registry.access.redhat.com"
      ],
      "packagePatterns": [
        "^ubi9/ubi-minimal$"
      ],
      "updateTypes": [
        "version"
      ]
    }
  ]
}
